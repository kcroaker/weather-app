{
    "kind":"ReplicationController",
    "apiVersion":"v1",
    "metadata":{
       "name":"om-weather-app",
       "labels":{
          "app":"om-weather-app"
       }
    },
    "spec":{
       "replicas":3,
       "selector":{
          "app":"om-weather-app"
       },
       "template":{
          "metadata":{
             "labels":{
                "app":"om-weather-app"
             }
          },
          "spec":{
             "containers":[
                {
                   "name":"om-weather-app",
                   "image":"123564074355.dkr.ecr.us-east-1.amazonaws.com/om-weather-app:v1",
                   "ports":[
                      {
                         "name":"http-server",
                         "containerPort":3000
                      }
                   ]
                }
             ]
          }
       }
    }
 }
 